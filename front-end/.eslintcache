ction(dispatch) {\r\n        \r\n        const response = await axios({method: \"POST\",\r\n                                      url: `${API_URL}/token`, \r\n                                      data: {username: data.username, \r\n                                             password: data.password},\r\n                                    })\r\n        localStorage.setItem(\"token\", response.data.token)\r\n        const { username } = localStorage.getItem(\"token\")\r\n        localStorage.setItem(\"username\", username)\r\n        return dispatch(getToken(response.data))\r\n    }\r\n}\r\n\r\nfunction getToken(data) {\r\n\r\n    return { \r\n        type: GET_TOKEN,\r\n        data\r\n    }\r\n}\r\n\r\nexport function registerUserWithApi(data){\r\n    return async function(dispatch) {\r\n        const response = await axios({method: \"POST\",\r\n                                      url: `${API_URL}/register`, \r\n                                      data: {username: data.username, \r\n                                             password: data.password,\r\n                                             firstName: data.firstName,\r\n                                             lastName: data.lastName},\r\n                                    })\r\n                                            \r\n        localStorage.setItem(\"token\", response.data.token)\r\n        const { username } = localStorage.getItem(\"token\")\r\n        localStorage.setItem(\"username\", username)\r\n        return dispatch(registerUser(response.data))\r\n    }\r\n}\r\n\r\nfunction registerUser(data) {\r\n    return { \r\n        type: REGISTER_USER,\r\n        data\r\n    }\r\n}\r\n\r\nexport function logout() {\r\n    return {\r\n        type: LOG_OUT\r\n    }\r\n}\r\n\r\n\r\n\r\n\r\n\r\n","C:\\Users\\kinga\\documents\\springboard-course\\capstone2\\beervana\\front-end\\src\\components\\UserList.js",["193"],"C:\\Users\\kinga\\documents\\springboard-course\\capstone2\\beervana\\front-end\\