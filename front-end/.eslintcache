ingFromApi(username))\r\n    }, [dispatch])\r\n\r\n\r\n    function addFollow(id) {\r\n        dispatch(addFollowWithApi(username, id))\r\n        dispatch(getFollowingFromApi(username))\r\n\r\n    }\r\n\r\n    function unFollow(id) {\r\n        dispatch(deleteFollowFromApi(username, id))\r\n        dispatch(getFollowingFromApi(username))\r\n    }\r\n\r\n\r\n\r\n    return (\r\n        <div>\r\n            <h3>Users:</h3>\r\n            {users.length > 0 ? \r\n            <ul className=\"list-group\">\r\n                {users.map(data => (\r\n                    <li className=\"list-group-item m-2\"key={data.id}><h3>{data.username}</h3>\r\n                    {followingIds.has(data.id) ? <button className=\"btn btn-primary btn-sm m-2\" onClick={() => unFollow(data.id)}>Unfollow</button> : <button className=\"btn btn-primary btn-sm\" onClick={() => addFollow(data.id)}>Follow</button>}\r\n                    </li>\r\n                ))}\r\n            </ul>\r\n            : <h5>No Users</h5>}\r\n        </div>\r\n    )\r\n}\r\n\r\n\r\n\r\n\r\nexport default UserList;","C:\\Users\\kinga\\documents\\springboard-course\\capstone2\\beervana\\front-end\\src\\components\\BeerList.js",[],"C:\\Users\\kinga\\documents\\springboard-course\\capstone2\\beervana\\front-end\\src\\components\\Post.js",[],"C:\\Users\\kinga\\documents\\springboard-course\\capstone2\\beervana\\front-end\\src\\components\\Beer.js",[],"C:\\Users\\kinga\\documents\\springboard-course\\capstone2\\beervana\\front-end\\src\\components\\Review.js",[],"C:\\Users\\kinga\\documents\\springboard-course\\capstone2\\beervana\\front-end\\src\\components\\BreweryList.js",[],"C:\\Users\\kinga\\documents\\springboard-course\\capstone2\\beervana\\front-end\\src\\components\\Brewery.js",[],"C:\\Users\\kinga\\documents\\springboard-course\\capstone2\\beervana\\front-end\\src\\components\\PostList.js",["182","183"],"C:\\Users\\kinga\\documents\\springboard-course\\capstone2\\beervana\\front-end\\src\\components\\ReviewLis